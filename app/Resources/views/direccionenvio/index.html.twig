{% extends 'DGInventarioBundle:Layout:layout.html.twig' %}

{% block contenido %}
    <h1>DireccionEnvio list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Direccion1</th>
                <th>Direccion2</th>
                <th>Ciudad</th>
                <th>Provinciaestado</th>
                <th>Postalzip</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for direccionEnvio in direccionEnvios %}
            <tr>
                <td><a href="{{ path('admin_direccionenvio_show', { 'id': direccionEnvio.id }) }}">{{ direccionEnvio.id }}</a></td>
                <td>{{ direccionEnvio.direccion1 }}</td>
                <td>{{ direccionEnvio.direccion2 }}</td>
                <td>{{ direccionEnvio.ciudad }}</td>
                <td>{{ direccionEnvio.provinciaEstado }}</td>
                <td>{{ direccionEnvio.postalZip }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_direccionenvio_show', { 'id': direccionEnvio.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_direccionenvio_edit', { 'id': direccionEnvio.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_direccionenvio_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
