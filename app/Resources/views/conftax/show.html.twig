{% extends 'base.html.twig' %}

{% block body %}
    <h1>ConfTax</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ confTax.id }}</td>
            </tr>
            <tr>
                <th>Nombretax</th>
                <td>{{ confTax.nombreTax }}</td>
            </tr>
            <tr>
                <th>Numeroregistro</th>
                <td>{{ confTax.numeroRegistro }}</td>
            </tr>
            <tr>
                <th>Porcentaje</th>
                <td>{{ confTax.porcentaje }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_conftax_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_conftax_edit', { 'id': confTax.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
