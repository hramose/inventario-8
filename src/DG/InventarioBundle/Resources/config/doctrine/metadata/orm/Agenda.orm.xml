<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Agenda" table="agenda">
    <indexes>
      <index name="fk_agenda_usuario_sistema1_idx" columns="usuario_sistema_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombres" type="string" column="nombres" length="100" nullable="true"/>
    <field name="apellidos" type="string" column="apellidos" length="100" nullable="true"/>
    <field name="fijo" type="string" column="fijo" length="25" nullable="true"/>
    <field name="celular" type="string" column="celular" length="25" nullable="true"/>
    <field name="cargo" type="string" column="cargo" length="100" nullable="true"/>
    <field name="fax" type="string" column="fax" length="45" nullable="true"/>
    <field name="email1" type="string" column="email1" length="100" nullable="true"/>
    <field name="email2" type="string" column="email2" length="100" nullable="true"/>
    <many-to-one field="usuarioSistema" target-entity="UsuarioSistema">
      <join-columns>
        <join-column name="usuario_sistema_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
