{% extends 'base.html.twig' %}

{% block body %}
    <h1>LocalidadContacto</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ localidadContacto.id }}</td>
            </tr>
            <tr>
                <th>Nombremanager</th>
                <td>{{ localidadContacto.nombreManager }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ localidadContacto.email }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ localidadContacto.telefono }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ localidadContacto.fax }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_localidadcontacto_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_localidadcontacto_edit', { 'id': localidadContacto.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
