{% extends 'DGInventarioBundle:Layout:layout.html.twig' %}

{% block contenido %}
    <h1>TimeZone list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for timeZone in timeZones %}
            <tr>
                <td><a href="{{ path('admin_timezone_show', { 'id': timeZone.id }) }}">{{ timeZone.id }}</a></td>
                <td>{{ timeZone.nombre }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_timezone_show', { 'id': timeZone.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_timezone_edit', { 'id': timeZone.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_timezone_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
