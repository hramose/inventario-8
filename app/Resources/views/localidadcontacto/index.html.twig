{% extends 'DGInventarioBundle:Layout:layout.html.twig' %}

{% block contenido %}
    <h1>LocalidadContacto list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombremanager</th>
                <th>Email</th>
                <th>Telefono</th>
                <th>Fax</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for localidadContacto in localidadContactos %}
            <tr>
                <td><a href="{{ path('admin_localidadcontacto_show', { 'id': localidadContacto.id }) }}">{{ localidadContacto.id }}</a></td>
                <td>{{ localidadContacto.nombreManager }}</td>
                <td>{{ localidadContacto.email }}</td>
                <td>{{ localidadContacto.telefono }}</td>
                <td>{{ localidadContacto.fax }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_localidadcontacto_show', { 'id': localidadContacto.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_localidadcontacto_edit', { 'id': localidadContacto.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_localidadcontacto_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
